{
    "name": "lequipe/mockserver-behat-context",
    "description": "Behat context for MockServer.",
    "type": "library",
    "license": "MIT",
    "require": {
        "behat/behat": "^3.0",
        "symfony/http-client": "^4.4|^5.0",
        "behat/gherkin": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Lequipe\\MockServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lequipe\\Test\\MockServer\\": "tests/"
        }
    }
}
